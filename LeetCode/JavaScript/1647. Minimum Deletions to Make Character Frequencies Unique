/**
 * @param {string} s
 * @return {number}
 */
var minDeletions = function(s) {
    let answer = 0;
    const arr = s.split('');
    const dict = {};
    const mem = new Array(10000);
    for(let i = 0; i < 10000; i++){
        mem[i] = false;
    }
    arr.map(char => {
        dict[char] = dict[char] ? dict[char] + 1 : 1;
    })
    const values = Object.values(dict);
    values.map(v => {
        if(mem[v]){
            let nextPos = v;
            while(nextPos > 0){
                if(!mem[nextPos]){
                    mem[nextPos] = !mem[nextPos];
                    answer += v - nextPos;
                    break;
                }else{
                    nextPos--;
                }
            }
            if(nextPos === 0){
                answer += v;
            }
        }else{
            mem[v] = true;
        }
    })
    return answer;
};
